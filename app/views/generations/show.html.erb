
<div class="page-header">
  <h1><%= @generation.name %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong>Nombre :</strong></dt>
  <dd><%= @generation.name %></dd>
  <dt><strong>Fecha Inicial:</strong></dt>
  <dd><%= @generation.start_date %></dd>
  <dt><strong>Creado en:</strong></dt>
  <dd><%= @generation.created_at.strftime("%d/%B/%Y %H:%M") %></dd>
  <dt><strong>Actualziado en:</strong></dt>
  <dd><%= @generation.updated_at.strftime("%d/%B/%Y %H:%M") %></dd>
</dl>

